<Feature xmlns="http://www.sila-standard.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Category="examples" FeatureVersion="1.0" MaturityLevel="Verified" Originator="org.silastandard" SiLA2Version="1.0" xsi:schemaLocation="http://www.sila-standard.org https://gitlab.com/SiLA2/sila_base/raw/master/schema/FeatureDefinition.xsd">
  <Identifier>Atlas</Identifier>
  <DisplayName>Atlas</DisplayName>
  <Description>A Sila_server for Atlas</Description>
  <Command>
    <Identifier>Recommend</Identifier>
    <DisplayName>Recommend</DisplayName>
    <Description>get recommendation from Atlas</Description>
    <Observable>Yes</Observable>
    <Parameter>
      <Identifier>Campaign</Identifier>
      <DisplayName>Campaign</DisplayName>
      <Description>Olympus Campaign Object</Description>
      <DataType>
        <Basic>Binary</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>Config</Identifier>
      <DisplayName>Config</DisplayName>
      <Description>Configuration for the optimizer</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Response>
      <Identifier>Termination</Identifier>
      <DisplayName>Termination</DisplayName>
      <Description>Termination message</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Response>
    <IntermediateResponse>
      <Identifier>Status</Identifier>
      <DisplayName>Status</DisplayName>
      <Description>Status of Campaign</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </IntermediateResponse>
    <IntermediateResponse>
      <Identifier>BinaryPayload</Identifier>
      <DisplayName>BinaryPayload</DisplayName>
      <Description>BinaryPayload</Description>
      <DataType>
        <Basic>Binary</Basic>
      </DataType>
    </IntermediateResponse>
    <IntermediateResponse>
      <Identifier>StringPayload</Identifier>
      <DisplayName>StringPayload</DisplayName>
      <Description>StringPayload</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </IntermediateResponse>
  </Command>
  <Property>
    <Identifier>Status</Identifier>
    <DisplayName>Status</DisplayName>
    <Description>Indicates the status of Atlas</Description>
    <Observable>No</Observable>
    <DataType>
      <Basic>String</Basic>
    </DataType>
  </Property>
</Feature>
